<!doctype html>
<!--[if lt IE 7]><html class="no-js ie ie6 lt-ie9 lt-ie8 lt-ie7" {{site.language_attributes}}> <![endif]-->
<!--[if IE 7]><html class="no-js ie ie7 lt-ie9 lt-ie8" {{site.language_attributes}}> <![endif]-->
<!--[if IE 8]><html class="no-js ie ie8 lt-ie9" {{site.language_attributes}}> <![endif]-->
<!--[if gt IE 8]><!--><html class="no-js" {{site.language_attributes}}> <!--<![endif]-->
<head>
    <meta charset="{{site.charset}}" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>{{wp_title}}</title>

    <link rel="pingback" href="{{site.pingback_url}}" />

    {{function('wp_head')}}

    {% block head %}
    {% endblock %}

    <link href="https://fonts.googleapis.com/css?family=Montserrat:Light,Regular|Oswald:Light,Extra-Light,Light,Regular" rel="stylesheet">

    <!--[if lt IE 9]>
    <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    <style>
        {{ options.css_override }}
    </style>

</head>

<body class="{{body_class}}" data-source="base.twig">

<header class="site-header" role="banner">

        <nav class="">
            <div class="">

                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="" id="">
                    <div class="">
                        {{function("get_search_form")}}
                    </div>
                    <ul class="">
                        {% for item in menu.get_items %}
                            <li class="{{item.classes | join(' ')}}">
                                <a class="" href="{{item.get_link}}">{{item.title}}</a>
                                {% if item.get_children %}
                                    <ul class="">
                                        {% for child in item.get_children %}
                                            <li class="">
                                                <a href="{{child.get_link}}">{{child.title}}</a>
                                            </li>
                                        {% endfor %}
                                    </ul>
                                {% endif %}
                            </li>
                        {% endfor %}
                    </ul>

                </div><!-- /.navbar-collapse -->
            </div><!-- /.container-fluid -->
        </nav>

</header>